<link rel="stylesheet" href="/css/add_newEmployee.css">

<div class="signUp-body-conatiner">
    <div id="sign-up-conatiner">
        <h2>ADD NEW EMPLOYEE</h2>
        <form action="/users/createAccount" method="POST">
            <div>
                <div>
                    <label for="name">Name</label>
                </div>
                <input type="text" id="name" name="name" placeholder="Enter your name" required>
            </div>
            <div>
                <div>
                    <label for="email">Email</label>
                </div>
                <input type="email" id="email" name="email" placeholder="abc@email.com" required>
            </div>
            <div>
                <div>
                    <label for="password">Password</label>
                </div>
                <input type="password" id="password" name="password" required>
            </div>
            <div>
                <div>
                    <label for="Cnfpassword">Confirm Password</label>
                </div>
                <input type="password" id="Cnfpassword" name="Cnfpassword" required>
            </div>
            <div>
                <button type="submit">Create Employee</button>
            </div>
        </form>
    </div>
</div>